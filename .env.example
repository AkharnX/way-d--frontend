# Environment variables for Way-d Frontend
# Copy this file to .env.local and update the values

# Application Information
VITE_APP_VERSION=1.0.0
VITE_BUILD_DATE=2024-12-22
VITE_APP_NAME=Way-d

# Features Configuration
VITE_ANALYTICS_ENABLED=true
VITE_PREMIUM_ENABLED=true
VITE_EVENTS_ENABLED=true
VITE_NOTIFICATIONS_ENABLED=true

# Contact Information
VITE_SUPPORT_EMAIL=support@way-d.com
VITE_HELP_URL=https://way-d.com/help
VITE_PRIVACY_URL=https://way-d.com/privacy
VITE_BUG_REPORT_URL=https://way-d.com/bug-report

# API Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_BACKEND_TIMEOUT=10000

# Development Settings
VITE_DEBUG_MODE=false
VITE_MOCK_DATA=false

# Localization
VITE_DEFAULT_LOCALE=fr-CI
VITE_DEFAULT_CURRENCY=XOF
VITE_DEFAULT_TIMEZONE=Africa/Abidjan

# Profile Limits
VITE_MAX_PHOTOS=6
VITE_MAX_INTERESTS=10
VITE_MAX_BIO_LENGTH=500
VITE_FREE_SWIPES_PER_DAY=10
